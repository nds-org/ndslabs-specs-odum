{
    "key": "tworavens",
	"access":"external",
    "description": "A collection of statistical tools for data exploration, analysis, and meta-analysis.",
    "label": "TwoRavens",
	"info": "https://nationaldataservice.atlassian.net/wiki/display/NDSC/Two+Ravens",
	"logo" : "https://nationaldataservice.atlassian.net/wiki/download/attachments/17170437/tworavens-small.png",
	"display" : "stack",
    "image": {
		"name": "ndslabs/tworavens",
		"tags": ["latest", "4.2.3"]
	},
    "ports": [
        { "port": 80, "protocol": "http" }
    ],
    "resourceLimits": {
        "cpuMax": 200,
        "cpuDefault": 100,
        "memMax": 1000,
        "memDefault": 500
    },
    "config":  [
        {
            "name": "DATAVERSE_URL",
            "value": "https://$(NDSLABS_STACK)-dataverse.$(NDSLABS_DOMAIN)",
            "canOverride": false
        },
        {
            "name": "TWORAVENS_URL",
            "value": "https://$(NDSLABS_STACK)-tworavens.$(NDSLABS_DOMAIN)",
            "canOverride": false
        }
    ],
	"repositories" : [{
		"type" : "git",
		"url" : "https://github.com/IQSS/TwoRavens"
	}],
    "readinessProbe" : {
        "type" : "http",
        "path" : "/dataexplore/gui.html",
        "port" : 80,
        "initialDelay": 10,
        "timeout" : 120
    },
	"developerEnvironment": "cloud9nodejs",
	"tags" : ["21", "28"]
}

