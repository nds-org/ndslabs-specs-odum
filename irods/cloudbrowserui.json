{
    "key": "cloudbrowserui",
    "label": "iRODS Cloudbrowser",
	"description" : "Web-based interface to any iRODS grid",
    "image": {
		"name": "ndslabs/cloudbrowser-ui",
		"tags": ["4.1.3","latest"]
	},
	"access": "external",
	"display": "standalone",
    "info": "https://nationaldataservice.atlassian.net/wiki/display/NDSC/IRODS",
    "logo": "https://nationaldataservice.atlassian.net/wiki/download/attachments/15532044/irods-small.png",
    "ports": [
        {
            "port": 80,
            "protocol": "http"
        }
    ],
    "depends": [
      { "key": "cloudbrowser", "required": true }
    ],
    "readinessProbe" : {
        "type" : "http",
        "path" : "app/images/beta_test_logo.png",
        "port" : 80,
        "initialDelay": 10,
        "timeout" : 600
    },
	"repositories": [
		{
			"type" : "git", 
			"url" : "https://github.com/DICE-UNC/irods-cloud-browser"
		}
	],
    "resourceLimits": {
        "cpuMax": 250,
        "cpuDefault": 100,
        "memMax": 250,
        "memDefault": 100
    },
	"tags" : ["6", "16"]
}
